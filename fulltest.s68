# SECTION 23: Complex Nested Structures
# ============================================
print "=== Complex Nested Logic ==="

1300 = "apple,BANANA,Cherry,DATE,elderberry"
1301 = split(1300, ",")

foreach item in 1301 do
    1310 = uppercase(item)
    1311 = lowercase(item)
    
    if 1310 equals item then
        1320 = "Was uppercase: "
        1321 = concat(1320, 1311)
        print 1321
    else
        if 1311 equals item then
            1330 = "Was lowercase: "
            1331 = concat(1330, 1310)
            print 1331
        else
            1340 = "Mixed case: "
            1341 = concat(1340, item)
            print 1341
        endif
    endif
endfor

