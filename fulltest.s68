# ============================================
# SECTION 1: Basic Assignment and Printing
# ============================================
print "=== Basic Operations ==="

1 = "Hello, strings68!"
print 1

2 = "Welcome to string-only programming"
print 2

# ============================================
# SECTION 2: String Transformations
# ============================================
print "=== String Transformations ==="

10 = "testing uppercase"
11 = uppercase(10)
print 11

20 = "TESTING LOWERCASE"
21 = lowercase(20)
print 21

30 = "reverse this text"
31 = reverse(30)
print 31

40 = "count me"
41 = length(40)
print "Length: "
print 41

# ============================================
# SECTION 3: String Concatenation
# ============================================
print "=== Concatenation ==="

50 = "Hello"
51 = " "
52 = "World"
53 = concat(50, 51)
54 = concat(53, 52)
print 54

# ============================================
# SECTION 4: String Replacement
# ============================================
print "=== String Replace ==="

60 = "The cat sat on the mat"
61 = replace(60, "cat", "dog")
print 61

62 = replace(60, "mat", "floor")
print 62

# ============================================
# SECTION 5: Substring Extraction
# ============================================
print "=== Substring ==="

70 = "Hello World"
71 = substring(70, "0", "5")
print 71

72 = substring(70, "6", "5")
print 72

# ============================================
# SECTION 6: Pattern Matching
# ============================================
print "=== Pattern Matching ==="

80 = "Order #12345 costs $99.99"
81 = match(80, "[#][0-9]+")
print 81

82 = match(80, "[$][0-9]+[.][0-9]+")
print 82

90 = "Email: john.doe@example.com"
91 = match(90, "[a-z]+[.][a-z]+[@][a-z]+[.][a-z]+")
print 91

100 = "Phone: 555-1234"
101 = match(100, "[0-9]+-[0-9]+")
print 101

# ============================================
# SECTION 7: Split and Array Operations
# ============================================
print "=== Split and Array Access ==="

110 = "apple,banana,cherry,date"
111 = split(110, ",")

112 = get(111, "0")
print 112

113 = get(111, "1")
print 113

114 = get(111, "2")
print 114

115 = get(111, "3")
print 115

# Split with empty delimiter (split into characters)
120 = "hello"
121 = split(120, "")
122 = get(121, "0")
123 = get(121, "1")
print 122
print 123

# ============================================
# SECTION 8: Conditionals - Equals
# ============================================
print "=== Conditionals: equals ==="

200 = "apple"
201 = "apple"
202 = "banana"

if 200 equals 201 then
    print "200 equals 201: TRUE"
endif

if 200 equals 202 then
    print "200 equals 202: TRUE"
else
    print "200 equals 202: FALSE"
endif

# ============================================
# SECTION 9: Conditionals - Contains
# ============================================
print "=== Conditionals: contains ==="

210 = "hello world"
211 = "world"
212 = "xyz"

if 210 contains 211 then
    print "Found 'world' in string"
endif

if 210 contains 212 then
    print "Found 'xyz' in string"
else
    print "'xyz' not found"
endif

# ============================================
# SECTION 10: Conditionals - Starts/Ends With
# ============================================
print "=== Conditionals: startswith/endswith ==="

220 = "Hello World"
221 = "Hello"
222 = "World"

if 220 startswith 221 then
    print "Starts with 'Hello'"
endif

if 220 endswith 222 then
    print "Ends with 'World'"
endif

# ============================================
# SECTION 11: Character Iteration
# ============================================
print "=== Character Iteration ==="

300 = "ABC"
foreach char in 300 do
    301 = lowercase(char)
    print 301
endfor

# ============================================
# SECTION 12: Array Iteration
# ============================================
print "=== Array Iteration ==="

310 = "red,green,blue"
311 = split(310, ",")

foreach color in 311 do
    312 = uppercase(color)
    print 312
endfor

# ============================================
# SECTION 13: Nested Conditionals
# ============================================
print "=== Nested Conditionals ==="

400 = "password123"
401 = "123"
402 = "pass"

if 400 contains 401 then
    print "Contains numbers"
    if 400 contains 402 then
        print "Also contains 'pass'"
    endif
endif

# ============================================
# SECTION 14: Conditionals in Loops
# ============================================
print "=== Conditionals in Loops ==="

410 = "apple,BANANA,cherry,DATE"
411 = split(410, ",")

foreach fruit in 411 do
    412 = uppercase(fruit)
    413 = lowercase(fruit)
    
    if 412 equals fruit then
        print "Already uppercase"
    else
        print 412
    endif
endfor

